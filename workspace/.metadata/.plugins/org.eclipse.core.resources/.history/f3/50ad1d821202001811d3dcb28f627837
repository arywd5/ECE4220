#include <stdio.h>
#include <stdlib.h>
#include <wiringPi.h>

#define RED_LED 2;
#define BLUE_LED 5;

int main(void) {

	wiringPiSetupGpio();

	pinMode(RED_LED, OUTPUT);
	pinMode(BLUE_LED, OUTPUT);

	while(1){
		digitalWrite(RED_LED, 1);
		sleep(1);
		digitalWrite(RED_LED, 0);
		digitalWrite(BLUE_LED, 1);
		sleep(1);
		digitalWrite(BLUE_LED, 0);

	}


	return 0;
}
